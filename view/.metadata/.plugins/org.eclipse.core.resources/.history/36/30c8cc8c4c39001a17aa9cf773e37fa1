          <ownedTools xsi:type="tool:NodeCreationDescription" name="Add Artifact" precondition="aql:self.eClass().name='LegacySourceCode' or self.eClass().name='TransformedSourceCode' or self.eClass().name='LPSM' or self.eClass().name='TPSM'" nodeMappings="//@ownedViewpoints[name='MEFiSTo']/@ownedRepresentations[name='Horseshoe%20Model']/@defaultLayer/@containerMappings[name='Outer%20Artifact']/@subNodeMappings[name='Artifact']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:Switch">
                <cases conditionExpression="aql:self.eClass().name = 'LPSM'">
                  <subModelOperations xsi:type="tool_1:CreateInstance" typeName="transformation.LPSM" referenceName="subArtifact">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="New LPSM Model"/>
                  </subModelOperations>
                </cases>
                <cases conditionExpression="aql:self.eClass().name = 'TPSM'">
                  <subModelOperations xsi:type="tool_1:CreateInstance" typeName="transformation.TPSM" referenceName="subArtifact">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="New TPSM Model"/>
                  </subModelOperations>
                </cases>
                <cases conditionExpression="aql:self.eClass().name = 'TransformedSourceCode'">
                  <subModelOperations xsi:type="tool_1:CreateInstance" typeName="transformation.TransformedSourceCode" referenceName="subArtifact">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="New Transformed Source Code"/>
                  </subModelOperations>
                </cases>
                <cases conditionExpression="aql:self.eClass().name = 'LegacySourceCode'">
                  <subModelOperations xsi:type="tool_1:CreateInstance" typeName="transformation.LegacySourceCode" referenceName="subArtifact">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="name"/>
                  </subModelOperations>
                </cases>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="Add Activity" precondition="aql:self.eClass().name='ModelDiscovery' or self.eClass().name='Enrichment' or self.eClass().name='LanguageTransformation' or self.eClass().name='CodeGeneration'" nodeMappings="//@ownedViewpoints[name='MEFiSTo']/@ownedRepresentations[name='Horseshoe%20Model']/@defaultLayer/@containerMappings[name='Outer%20Activity']/@subNodeMappings[name='Activity']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:Switch">
                <cases conditionExpression="aql:self.eClass().name = 'ModelDiscovery'">
                  <subModelOperations xsi:type="tool_1:CreateInstance" typeName="transformation.ModelDiscovery" referenceName="subActivity">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="New Model Discovery Activity"/>
                  </subModelOperations>
                </cases>
                <cases conditionExpression="aql:self.eClass().name = 'Enrichment'">
                  <subModelOperations xsi:type="tool_1:CreateInstance" typeName="transformation.Enrichment" referenceName="subActivity">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="New Enrichment Activity"/>
                  </subModelOperations>
                </cases>
