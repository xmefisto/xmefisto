                  <subModelOperations xsi:type="tool_1:CreateInstance" typeName="transformation.LanguageTransformation" referenceName="subActivity">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="New Language Transformation Activity"/>
