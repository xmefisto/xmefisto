      <validationSet name="MEFiSTo">
        <ownedRules xsi:type="validation:SemanticValidationRule" level="ERROR" message="No input has been defined for this activity" targetClass="fragment.Activity">
          <audits auditExpression="aql:not (self.subActivity->isEmpty()) or not (self.input->isEmpty())"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" level="ERROR" message="No output has been defined for this activity" targetClass="fragment.Activity">
          <audits auditExpression="aql:not (self.subActivity->isEmpty()) or not (self.output->isEmpty())"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" level="ERROR" message="The artifact is not used as an input" targetClass="fragment.Artifact">
          <audits auditExpression="aql:not (self.subArtifact->isEmpty()) or not (self.inputTo->isEmpty()) or (self.eClass().name='LegacySourceCode' and self.getsRemoved) or (self.eClass().name='TransformedSourceCode')"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" level="ERROR" message="Model Discovery can only output an LPSM" targetClass="transformation.ModelDiscovery">
          <audits auditExpression="aql:self.output->forAll(inp | inp.eClass().name = 'LPSM')"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" level="ERROR" message="The input to Model Discovery can only be Legacy Source Code, a System or a Platform" targetClass="transformation.ModelDiscovery">
          <audits auditExpression="aql:self.input->forAll(inp | inp.eClass().name = 'LegacySourceCode' or inp.eClass().name = 'System' or inp.eClass().name = 'Platform')"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" level="ERROR" message="This artifact is not an output of any activity" targetClass="fragment.Artifact">
          <audits auditExpression="aql:not (self.subArtifact->isEmpty()) or not (self.outputFrom->isEmpty()) or (self.eClass().name='LegacySourceCode') or (self.eClass().name='System') or (self.eClass().name='Platform')"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" level="ERROR" message="Language Transformation can only output a TPSM" targetClass="transformation.LanguageTransformation">
          <audits auditExpression="aql:self.output->forAll(inp | inp.eClass().name = 'TPSM')"/>
        </ownedRules>
      </validationSet>
